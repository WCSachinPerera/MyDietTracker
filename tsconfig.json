{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "paths": {
      "@firebase/auth": [
        "./node_modules/@firebase/auth/dist/index.rn.d.ts"
      ],
      "@/*": [
        "./*"
      ]
    },
    "jsx": "react",
    "types": [
      "react-native",
      "node",
      "@react-native-firebase/auth"
    ],
    "strict": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}